{
  "manifest_version": 2,
  "name": "Facepage",
  "description": "This extension shows concepts and entities for the current page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "http://localhost:8000/*",
    "https://ajax.googleapis.com/",
    "http://*.google.com/"
  ]
}